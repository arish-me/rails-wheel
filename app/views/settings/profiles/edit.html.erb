<% content_for :settings_tab do %>
  <%= render 'settings/settings_tab' %>
<% end %>

<section class="bg-white dark:bg-gray-900 rounded-lg p-4">
  <div class="py-4 px-2 lg:py-8">
  <h2 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">Profile</h2>
  <p class="text-gray-500 dark:text-gray-400 mb-4">Update your Profile for Better Visibility.</p>

  <hr class="h-px mb-4 bg-gray-200 border-0 dark:bg-gray-700">

  <%= render "form", profile: @profile %>
</div>
</section>
