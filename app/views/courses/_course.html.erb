<div id="<%= dom_id course %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= course.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Subtitle:</strong>
    <%= course.subtitle %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= course.description %>
  </p>

</div>
